# type: textblock
#~ msgid "B<dh_desktop> [S<I<debhelper options>>] [B<-p<package>>]"
#~ msgstr "B<dh_desktop> [S<I<opciones debhelper>>] [B<-p<paquete>>]"

# type: textblock
